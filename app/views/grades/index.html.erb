<p id="notice"><%= notice %></p>

<h1>Listing Grades</h1>

<table>
  <thead>
    <tr>
      <th>Homework</th>
      <th>User</th>
      <th>P1</th>
      <th>P2</th>
      <th>P3</th>
      <th>P4</th>
      <th>P5</th>
      <th>P6</th>
      <th>P7</th>
      <th>P8</th>
      <th>P9</th>
      <th>P10</th>
      <th>Note</th>
      <th>Voto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grades.each do |grade| %>
      <tr>
        <td><%= grade.homework_id %></td>
        <td><%= grade.user_id %></td>
        <td><%= grade.p1 %></td>
        <td><%= grade.p2 %></td>
        <td><%= grade.p3 %></td>
        <td><%= grade.p4 %></td>
        <td><%= grade.p5 %></td>
        <td><%= grade.p6 %></td>
        <td><%= grade.p7 %></td>
        <td><%= grade.p8 %></td>
        <td><%= grade.p9 %></td>
        <td><%= grade.p10 %></td>
        <td><%= grade.note %></td>
        <td><%= grade.voto %></td>
        <td><%= link_to 'Show', grade %></td>
        <td><%= link_to 'Edit', edit_grade_path(grade) %></td>
        <td><%= link_to 'Destroy', grade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grade', new_grade_path %>
